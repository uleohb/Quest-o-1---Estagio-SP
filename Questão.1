#include <stdio.h>

int main() {
    int indice = 13, soma = 0, k = 0;

    //enquanto k for menor que indice
    while (k < indice) {
        k = k + 1;
        soma = soma + k;
    } 
    //imprima o valor de ''soma''
        printf("O valor de soma e: \n");
        printf("%d", soma);
        
    return 0;
}
